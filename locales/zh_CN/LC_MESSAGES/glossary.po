msgid "stringstrings"
msgstr ""

#: ../glossary.rst:133
msgid "Normal text. Mind the length limits that apply."
msgstr ""

#: ../glossary.rst:134
msgid "themetheme id"
msgstr ""

#: ../glossary.rst:137
msgid "The name of a theme entry defined in :file:`thememd.ini`."
msgstr ""

